//  User defined method

#include<bits/stdc++.h>
using namespace std;

int product(int a, int b)
{
	return a*b;
}
int main()
{
	int n1,n2;
	cin>>n1>>n2;
	
	cout<<"The product of n1 and n2 is : "<<product(n1,n2);
}
